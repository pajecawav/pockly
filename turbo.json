{
	"pipeline": {
		"build": {
			"outputs": [".next/**"],
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false
		},
		"generate": {
			"cache": false
		},
		"lint": {
			"dependsOn": ["^lint"],
			"outputs": []
		},
		"typecheck": {
			"dependsOn": ["^typecheck"],
			"outputs": []
		},
		"clean": {
			"cache": false
		}
	}
}
